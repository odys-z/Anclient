{
  "name": "anprism",
  "displayName": "Anprism",
  "description": "Anclient's js page sandbox",
  "version": "0.2.7",
  "icon": "res/anprism.png",
  "publisher": "ody-zhou",
  "repository": {
    "type": "git",
    "url": "https://github.com/odys-z/Anclient/tree/master/js/anprism"
  },
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:anprism.load",
    "onCommand:anprism.restartServer"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "anprism.load",
        "title": "Load Anprism",
        "category": "Anprism"
      },
      {
        "command": "anprism.refresh",
        "title": "Refresh Anprism",
        "category": "Anprism"
      },
      {
        "command": "anprism.restartServer",
        "title": "Start server",
        "category": "Anprism"
      },
      {
        "command": "anprism.shutdownServer",
        "title": "Shutdown server",
        "category": "Anprism"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceLangId == html",
          "command": "anprism.load",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npx webpack",
    "watch": "npx webpack --watch",
    "package": "npx webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p -w . --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.2.2",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.4",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.1"
  }
}
